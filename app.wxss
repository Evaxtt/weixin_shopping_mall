/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 分割竖线 */
.bd-r{
  position: relative;
}
.bd-r::after{
  content: "";
  width: 1rpx;
  height: 100%;
  background-color: #ccc;
  position: absolute;
  right: 0rpx;
  top: 0;
}
/* 分隔行 */
.divider{
  height: 20rpx;
  background-color: #f1f1f1;
  box-shadow: 0 10rpx 10rpx rgba(0, 0,0, 0.1);
}
/* 文字超出部分隐藏，显示省略号 */
.ellipsis{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
/* 加载更多 */
.loadmore{
            color:#888;
            font-size: 16px;
            text-align: center;
        }

        .loading:before{
            content: "";
            width: 30px;
            height: 30px;
            margin-top: -5px;
            margin-right: 10px;
            display: inline-block;
            vertical-align: middle;
            animation: loading 1s steps(12) infinite;
            background:transparent url(data:image/jpeg;base64,) no-repeat;
            background-size: 100%;
        }

        @keyframes loading{
            from{
                transform: rotate(0deg);
            }
            to{
                transform: rotate(360deg);
            }
        }